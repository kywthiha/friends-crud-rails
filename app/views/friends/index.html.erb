

<h1>Friends</h1>

<div id="friends">
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Twitter</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
  <% @friends.each do |friend| %>
    <%= render friend %>
  <% end %>
  </tbody>
  </table>
  <div class="apple_pagination">
    <%= will_paginate @friends, :container => false %>
  </div>
</div>
